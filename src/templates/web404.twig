{% extends '_layouts/elementindex' %}
{% set title = '404 web requests' %}
{% set elementType = 'frontwise\\monitor404\\elements\\Web404' %}

{% block initJs %}
    Craft.elementIndex = Craft.createElementIndex('{{ elementType|e("js") }}', $('#main'), {
        context:        '{{ context }}',
        storageKey:     'elementindex.{{ elementType|e("js") }}',
        criteria:       { url: '{{ url }}', siteId: {{ siteId }} },
        hideSidebar:    true
    });
{% endblock %}